{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(?P<path>.*)\\.html$",
      "dest": "/$path.html"
    },
    {
      "src": "^/(?P<path>.*)$",
      "dest": "/$path"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
